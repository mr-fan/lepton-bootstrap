		</a>
		<!-- BEGIN show_section_block -->
		<a style="{DISPLAY_MANAGE_SECTIONS}" href="{ADMIN_URL}/pages/sections.php?page_id={PAGE_ID}" class="btn btn-info btn-primary" rel="tooltip" data-placement="bottom" data-original-title="{TEXT_MANAGE_SECTIONS}">
			<i class="icon-tasks"></i> {TEXT_MANAGE_SECTIONS}
		</a>		
		<!-- END show_section_block -->
