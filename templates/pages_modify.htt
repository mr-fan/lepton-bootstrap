<!-- BEGIN main_block -->
<div class="row-fluid">
	<div class="span4">
		<a data-content="
			<!-- BEGIN show_modify_block -->
			{LAST_MODIFIED} {MODIFIED_BY} ({MODIFIED_BY_USERNAME}), {MODIFIED_WHEN}
			<!-- END show_modify_block -->
			"
			href="#" title=""
			data-toggle="popover"
			data-html="true"
			class="popOver btn"
			data-original-title="{TEXT_CURRENT_PAGE}: <b>{PAGE_TITLE}</b> - ID: {PAGE_ID}"
